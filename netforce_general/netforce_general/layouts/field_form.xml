<form model="field">
    <field name="model_id"/>
    <field name="name"/>
    <field name="string"/>
    <field name="type"/>
    <field name="relation_id"/>
    <field name="relfield_id" condition='[["model_id","=",relation_id]]'/>
    <field name="selection"/>
    <field name="required"/>
    <field name="readonly"/>
    <field name="stored"/>
    <field name="function"/>
    <field name="default"/>
    <field name="search"/>
    <field name="condition"/>
    <field name="description"/>
</form>
